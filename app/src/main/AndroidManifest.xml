<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
  package="com.example.easypass">

  <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
  <uses-permission
    android:name="android.permission.CAMERA"
    android:required="true" />

  <application
    android:allowBackup="true"
    android:icon="@mipmap/main_new_icon_round"
    android:label="@string/app_name"
    android:roundIcon="@mipmap/main_new_icon"
    android:supportsRtl="true"
    android:theme="@style/Theme.EasyPass">
    <activity android:name=".InstructionsActivity"></activity>
    <activity android:name=".FinishProcess"></activity>
    <activity android:name=".ContactUs"></activity>
    <activity android:name=".StatusRequestActivity"></activity>
    <activity android:name=".AdvancedRegisterActivity"></activity>
    <activity android:name=".ValidationProcess"></activity>
    <activity android:name=".SplashScreenActivity">
      <intent-filter>
        <action android:name="android.intent.action.MAIN" />

        <category android:name="android.intent.category.LAUNCHER" />
      </intent-filter>
    </activity>
    <activity android:name=".ProcessActivity">
    </activity>
    <activity android:name=".MainActivity"></activity>
    <activity android:name=".SignUpActivity"></activity>
  </application>

</manifest>